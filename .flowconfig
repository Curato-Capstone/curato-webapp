[ignore]
.*node_modules/fbjs.*
.*node_modules/json5.*

[libs]
interfaces


[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
esproposal.decorators=ignore

module.name_mapper='^components\/\(.*\)$' -> '<PROJECT_ROOT>/client/js/components/\1'
module.name_mapper='^containers\/\(.*\)$' -> '<PROJECT_ROOT>/client/js/containers/\1'
module.name_mapper='^routes\/\(.*\)$' -> '<PROJECT_ROOT>/client/js/containers/routes/\1'
module.name_mapper='^modules\/\(.*\)$' -> '<PROJECT_ROOT>/client/js/modules/\1'
module.name_mapper='^js\/\(.*\)$' -> '<PROJECT_ROOT>/client/js/\1'
module.name_mapper='^stylesheets\/\(.*\)$' -> '<PROJECT_ROOT>/client/stylesheets/\1'
module.name_mapper='^utils\/\(.*\)$' -> '<PROJECT_ROOT>/client/js/utils/\1'